<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.company.calculator">

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" android:configChanges="orientation|screenSize">
        </activity>
        <activity android:name=".ProcentoweActivity" android:configChanges="orientation|screenSize">
        </activity>
        <activity android:name=".MoloweActivity" android:configChanges="orientation|screenSize">
        </activity>
        <activity android:name=".RoztworyActivity" android:configChanges="orientation|screenSize">
        </activity>
    </application>

</manifest>